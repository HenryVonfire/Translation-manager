<div class="header-menu">
  <div class="btn-group" role="group" aria-label="Basic example">
  <button class="btn btn-flat btn-warning" {{action 'openFile'}}>Open File</button>
  <button class="btn btn-flat btn-primary" {{action 'saveFile'}}>Save</button>
</div>
<br>
{{#if fileList}}
<div class="title">
keys
</div>
{{#each fileOrder as |file|}}
<div class="title">
{{file}}
</div>
{{/each}}
{{/if}}
</div>
<br><br><br>

{{#if renderList}}
<div class="file-render">
  {{obj-new-render
    obj=renderList
    save=(action 'save')
    removeKey=(action 'removeKey')
    addKey=(action 'addKey')}}
</div>
{{/if}}
