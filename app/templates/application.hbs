<button class="btn btn-flat btn-warning" {{action 'openFile'}}>Open File</button>
<button class="btn btn-flat btn-primary" {{action 'saveFile'}}>Save</button>
<hr>

{{#each objectList as |obj index|}}
  <div class="file-render">
    {{obj-render
      obj=obj
      objectIndex=index
      save=(action 'save')
      removeKey=(action 'removeKey')}}
  </div>
{{/each}}
