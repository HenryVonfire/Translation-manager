{{#each-in obj as |key value|}}
  {{#if (is-object (get-object-from-key obj key))}}
    <b>{{key}}:</b><br>
    {{obj-render obj=(get-object-from-key obj key) path=path lastKey=key}}
  {{else}}
    <div class="input-group">
      <span class="input-group-addon">{{key}}</span>
      {{one-way-input
      class="form-control"
      value=value
      update=(action 'save' path key value)}}
    </div>
  {{/if}}
{{/each-in}}
